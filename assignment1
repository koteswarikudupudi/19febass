#to count no of prime numbers in the given range
def prime(n):
    for i in range (2,n):
        if n%i==0:
            return False
    return True    
 
t=int(input())
for i in range(t):
    n=input().split()
    x=int(n[0])
    y=int(n[1])
    c=0
    for i in range(x+1,y):
        if prime(i):
           c=c+1
    print(c)        
